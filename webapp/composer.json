{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "packages/barpay",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "package",
            "package": {
                "name": "timvisee/laravel-has-many-sync",
                "version": "0.2.0",
                "source": {
                    "url": "git@github.com:timvisee/laravel-hasmany-sync.git",
                    "type": "git",
                    "reference": "07e398ba8df92c733124e4bb7e310b9f8d5be171"
                },
                "description": "Laravel has many sync",
                "type": "library",
                "require": {
                    "illuminate/support": "~6.2"
                },
                "license": "MIT",
                "authors": [
                    {
                        "name": "Alfa Adhitya",
                        "email": "alfa2159@gmail.com"
                    }
                ],
                "autoload": {
                    "psr-4": {
                        "Alfa6661\\EloquentHasManySync\\": "src"
                    }
                },
                "extra": {
                    "laravel": {
                        "providers": [
                            "Alfa6661\\EloquentHasManySync\\ServiceProvider"
                        ]
                    }
                },
                "minimum-stability": "dev",
                "prefer-stable": true
            }
        }
    ],
    "require": {
        "php": ">=7.3",
        "timvisee/barpay": "@dev",
        "timvisee/laravel-has-many-sync": "0.2.0",
        "barryvdh/laravel-debugbar": "^3.2.3",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "bunq/sdk_php": "^1.14.18",
        "intervention/validation": "^1.2",
        "laravel/framework": "^6.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^6.0",
        "mariuzzo/laravel-js-localization": "^1.4",
        "sentry/sentry-laravel": "^1.0.2",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "staudenmeir/eloquent-has-many-deep": "^1.8"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^8.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
