{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "packages/barpay",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "package",
            "package": {
                "name": "awssat/laravel-query-reset",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/awssat/laravel-query-reset.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.2",
        "awssat/laravel-query-reset": "dev-master",
        "alfa6661/laravel-has-many-sync": "^1.1",
        "barpay": "dev-76-payment-processing",
        "barryvdh/laravel-debugbar": "^3.2.3",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "bunq/sdk_php": "^1.12.1",
        "intervention/validation": "^1.2",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.8",
        "mariuzzo/laravel-js-localization": "^1.4",
        "sentry/sentry-laravel": "^1.0.2",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "staudenmeir/eloquent-has-many-deep": "^1.8"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^8.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
